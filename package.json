  "license": "UNLICENSED",
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "ethers": "^4.0.1",
    "express": "^4.16.3",
    "http-status": "^1.1.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.2.1",
    "mime-types": "^2.1.18",
    "moment": "^2.24.0",
    "multer": "^1.3.0",
    "mysql": "^2.16.0",
    "uuid": "^3.2.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/http-status": "^0.2.30",
    "@types/jest": "^23.3.2",
    "@types/joi": "^13.6.0",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/mime-types": "^2.1.0",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.3.6",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.1.2",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.9",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/.*?\\.d\\.ts$",
      "<rootDir>/.*?\\.mock\\.ts$"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ]
  }
}
